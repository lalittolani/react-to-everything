{
  "name": "react-to-everything",
  "version": "1.0.0",
  "description": "Starter Kit for simultaneously developing a react app for android, ios, desktop and web.",
  "author": "Mike Barkmin <mbarkmin@gmail.com> (http://barkmin.eu)",
  "license": "GPL-3.0",
  "keywords": [
    "mobile",
    "native",
    "immutable",
    "javascript",
    "react-native",
    "starter-kit",
    "webpack",
    "babel",
    "eslint",
    "es2015"
  ],
  "scripts": {
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "build": "grunt build",
    "coverage": "jest --coverage",
    "electron": "grunt serve-electron",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "lint": "eslint src",
    "package:android": "cd android && ./gradlew assembleRelease",
    "package:desktop": "grunt build && electron-packager dist/electron ReactToEveryThing --out dist --all && grunt clean:electronDist",
    "plop": "plop",
    "react-native": "node node_modules/react-native/local-cli/cli.js start",
    "start": "npm run web",
    "test": "jest",
    "travis": "npm run lint && npm test && npm run coverage",
    "web": "grunt serve-web",
    "web:dist": "grunt serve-web:dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikebarkmin/react-to-everything"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2 <4"
  },
  "src": ".",
  "dist": "dist",
  "dependencies": {
    "color": "~0.11.1",
    "grunt-exec": "^2.0.0",
    "i18n-js": "^1.0.0",
    "material-ui": "^0.17.0",
    "native-base": "^2.0.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-native": "^0.41.2",
    "react-native-i18n": "1.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "coveralls": "^2.11.16",
    "css-loader": "^0.26.1",
    "electron": "^1.4.13",
    "electron-packager": "^8.5.1",
    "eslint": "~3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "grunt": "^1.0.1",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^2.0.0",
    "grunt-open": "^0.2.3",
    "grunt-webpack": "^2.0.1",
    "jest": "^19.0.2",
    "jsdom": "^9.11.0",
    "load-grunt-tasks": "^3.5.2",
    "plop": "^1.7.4",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-native-cli": "^2.0.1",
    "react-native-mock": "0.3.1",
    "redux-logger": "^2.8.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-livereload-plugin": "^0.10.0"
  }
}
